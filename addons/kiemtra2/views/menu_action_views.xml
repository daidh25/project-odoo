<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <!-- ACTION -->
        <record id="action_phong" model="ir.actions.act_window">
            <field name="name">Phòng</field>
            <field name="res_model">phong</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="action_nhanvien" model="ir.actions.act_window">
            <field name="name">Nhân viên</field>
            <field name="res_model">nhanvien</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- MENU -->
        
           
        <menuitem id="menu_muc_goc" name="Ninh Văn Vượng">
            <menuitem
                id="menu_phong"
                name="Quản lý phòng"
                action="action_phong"/>
            <menuitem
                id="menu_nhanvien"
                name="Quản lý nhân viên"
                action="action_nhanvien"/>
        </menuitem>
    
    </data>
    

</odoo>
