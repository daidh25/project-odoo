<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data >
    
        <record id="view_nhanvien_tree" model="ir.ui.view">
            <field name="name">nhanvien.tree</field>
            <field name="model">nhanvien</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="empid"/>
                    <field name="gender"/>
                    <field name="phone"/>
                </tree>
            </field>
        </record>

        <record id="view_nhanvien_form" model="ir.ui.view">
            <field name="name">nhanvien.form</field>
            <field name="model">nhanvien</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="empid"/>
                        <field name="gender"/>
                        <field name="phone"/>
                    </group>
                </form>
            </field>
        </record>

    </data>
    
</odoo>
